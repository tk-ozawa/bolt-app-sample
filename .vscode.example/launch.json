{
  "version": "0.2.0",
  "configurations": [{
    "name": "Docker: Attach to Node",
    "type": "node",
    "request": "attach",
    "port": 9229,
    "address": "localhost",
    "localRoot": "${workspaceFolder}/dist",
    "remoteRoot": "/usr/src/app/dist",
    "protocol": "inspector",
    "outFiles": ["${workspaceFolder}/dist/**/*.js"],
    "trace": true,
    "restart": true
  }]
}